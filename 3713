class Solution {
public:
    int longestBalanced(string s) {
         vector<int> lastIndex(256, -1);
    int n = s.size();
    int left = 0;
    int maxLen = 0;
    for (int right = 0; right < n; right++) {
        if (lastIndex[s[right]] >= left) {
            left = lastIndex[s[right]] + 1;
        }
        
        lastIndex[s[right]] = right;
        maxLen = max(maxLen, right - left + 1);
    }
    
    return maxLen;
    }
};
